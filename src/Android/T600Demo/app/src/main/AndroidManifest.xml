<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.tijos.gas">

    <uses-permission android:name="android.permission.INTERNET" ></uses-permission>

    <application
        android:name="net.tijos.tikit.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="net.tijos.tikit.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.tijos.tikit.MainActivity" android:launchMode="singleInstance" />
        <service android:name="net.tijos.tikit.service.MonitorService" />
    </application>

</manifest>